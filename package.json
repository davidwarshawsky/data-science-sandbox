{
  "name": "immutable-regulatory-sandbox",
  "displayName": "Immutable Regulatory Sandbox",
  "description": "Trust-minimized environment for reproducible data science experiments.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "immutable.createSandbox",
        "title": "Immutable: Create Sandbox"
      },
      {
        "command": "immutable.finalizeExperiment",
        "title": "Immutable: Finalize Experiment"
      },
      {
        "command": "immutable.showDashboard",
        "title": "Immutable: Show Dashboard"
      },
      {
        "command": "immutable.showWelcome",
        "title": "Immutable: Show Welcome Guide"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "immutable-explorer",
          "title": "Immutable Sandbox",
          "icon": "resources/lock.svg"
        }
      ]
    },
    "views": {
      "immutable-explorer": [
        {
          "type": "webview",
          "id": "immutable.dashboardView",
          "name": "Dashboard"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/sqlite3": "^3.1.11",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "sqlite3": "^5.1.7"
  }
}